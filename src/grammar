expr: semicolon_expr

semicolon_expr: and_expr ; semicolon_expr
              | and_expr

and_expr: or_expr && and_expr
        | or_expr

or_expr: call_expr || or_expr
       | call_expr

call_expr: cmd_name [args...]
         | ( expr )
         | Empty

args: Word
    | DoubleQuotedString
    | SingleQuotedString
